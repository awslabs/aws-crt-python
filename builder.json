{
    "name": "aws-crt-python",
    "hosts": {
        "linux": {
            "architectures": {
                "x86": {
                    "enabled": false,
                    "image": "123124136734.dkr.ecr.us-east-1.amazonaws.com/aws-common-runtime/linux-python:x86"
                }
            }
        },
        "al2012": {
            "enabled": false
        },
        "manylinux": {
            "enabled": false
        },
        "windows": {
            "enabled": false
        }
    },
    "targets": {
        "android": {
            "enabled": false
        }
    },
    "upstream": [],
    "downstream": [],
    "build": [
        ["git", "submodule", "update", "--init", "--recursive"],
        ["{python}", "-m", "pip", "install", "."]
    ],
    "test": [
        ["{python}", "-m", "unittest", "discover"]
    ]
}
